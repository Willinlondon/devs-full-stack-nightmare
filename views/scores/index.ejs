<!DOCTYPE html>
<html>
	<head>
		<title>Scores</title>
		<link rel="stylesheet" href="/stylesheets/style.css" />
		<link
			rel="shortcut icon"
			type="image/png"
			href="./images/idlePlayer1CROPPED.png"
		/>
		<script src="https://cdn.jsdelivr.net/npm/p5@1.4.0/lib/p5.js"></script>
		<script type="text/javascript" src="javascripts/config.js"></script>
	</head>
	<body>
		<div id="page-container">
			<div id="page-header">
				<img src="./images/theGameText.png" , height="50" /> 
			  </div>

			  <div id="page-footer">
				<a href="/about" target="_blank"
				  ><img src="./images/createdByUs.png" , height="33"
				/></a>
			  </div>

			  <div id="main-area">
				<div id="left-sidebar"></div>
		
				<div id="play-area">
					
					<table style="width: 100%">
						<tr>
							<th>Player Name</th>
							<th>Player Score</th>
						</tr>
						<% scores.map((result)=>{ %>
						<tr>
							<td><%= result.username %></td>
							<td><%= result.score %></td>
						</tr>
						<% }) %>
					</table>
				</div>
		
				<div id="left"></div>
				<div id="right"></div>
				<div id="right-sidebar"></div>
			  </div>

		</div>

